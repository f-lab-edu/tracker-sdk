{
  "name": "tracker-sdk-nemo",
  "version": "1.0.0",
  "description": "트래픽 분석을 위한 SDK",
  "main": "dist/bundle.cjs",
  "module": "dist/bundle.js",
  "types": "dist/bundle.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "dev": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/cores/tracker.ts",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts --ext .ts,.js"
  },
  "keywords": [],
  "author": "nemo0824",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.1",
    "rollup": "^4.36.0",
    "rollup-plugin-dts": "^6.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "uuid": "^11.1.0"
  }
}
